    }}
}

local jsonData = HttpService:JSONEncode(data)

local function SendWebhook()
    if syn and syn.request then
        syn.request({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = jsonData
        })
        
        return true
    end
    
    if request then
        request({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = jsonData
        })
        return true
    end

    if http and http.request then
        http.request({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = jsonData
        })
        return true
    end

    if electron and electron.request then
        electron.request({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = jsonData
        })
        return true
    end
    if http_request then
        http_request({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = jsonData
        })
        return true
    end
    return false
end
local success = SendWebhook()
return success 
