local function GetHWID()
    local hwid = ""
    local success, result = pcall(function()
        return game:GetService("RbxAnalyticsService"):GetClientId()
    end)
    if success then
        hwid = result
    else
        hwid = "–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å HWID"
    end
    return hwid
end

local function GetCountry()
    local countryCode = ""
    local countryName = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"
    local countryFlag = "üè≥Ô∏è"
    
    local success, code = pcall(function()
        return LocalizationService:GetCountryRegionForPlayerAsync(LocalPlayer)
    end)
    if success and code then
        countryCode = code
        countryName = Countries[code] or "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"
        if #code == 2 then
            local firstLetter = string.byte(string.sub(code, 1, 1)) - string.byte("A") + 0x1F1E6
            local secondLetter = string.byte(string.sub(code, 2, 2)) - string.byte("A") + 0x1F1E6
            local firstChar = utf8.char(firstLetter)
            local secondChar = utf8.char(secondLetter)
            
            countryFlag = firstChar .. secondChar
        end
    end
    
    return countryCode, countryName, countryFlag
end

local profileUrl = "https://www.roblox.com/users/" .. LocalPlayer.UserId .. "/profile"

local placeId = game.PlaceId
local jobId = game.JobId

local countryCode, countryName, countryFlag = GetCountry()

